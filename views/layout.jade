!!!5
html(lang="en")
  head
    title a young programmer
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript', src='https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js')
    script(type='text/javascript', src='https://code.jquery.com/ui/1.10.3/jquery-ui.js')
    script(type='text/javascript', src='js/bounceThis.js')
    script.
       $(function() {
         $('.header').bounceThis({
            bounceHeight  : '20px',
            dropDownSpeed : 300,
            delay         : 400
          });
          $('.fade-in').fadeIn(1500,'swing');
       });
       $(function(){
          $( "#tabs").tabs();
       });
  body
    .header
        h1 <a href="/" STYLE="TEXT-DECORATION: NONE; color : #fff" > a young programmer </a>
    .fade-in
      .container
        h1<center><style="display:none"> Home of Michael Young</style></center>
        block content
    .fade-in
      .footer
        p.ltext Running on node with Express, Jade and Stylus
        p.rtext #{serv.curUsers.length} users online, #{serv.totalUsers.length} total users
        <div style="clear: both;"></div>